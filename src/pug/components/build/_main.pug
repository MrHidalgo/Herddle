// SECTION build main :: START

section#build-main.build-main

  .build-main__bg
    .build-main__bg-left
    .build-main__bg-right

  .build-main__wrapper

    .build-main__col
      h1.build-main__title.c-title!= "Built and designed for<br>Institutional Investors."

      .c-btn__wrapper
        a.build-main__btn.c-btn.c-btn--bg.c-btn--bg--irisBlue.c-btn--big(href='#' title='')
          span Get Started

      p.build-main__available
        span Available on
        i.icon-font.icon-mac
        span Desktop and
        i.icon-font.icon-phone
        span Mobile

    img.build-main__img(src='img/img-build-main.png' srcset='img/img-build-main@2x.png 2x')

// SECTION build main :: END